{
  "name": "n8n-nodes-imap-manager",
  "version": "0.3.0",
  "description": "n8n node to manage IMAP mail: search by Message-ID, add keywords, move, copy, delete.",
  "author": "James Forwood",
  "license": "MIT",
  "keywords": ["n8n-community-node","imap","imap-manager","imapflow"],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "postbuild": "cp nodes/imap-manager.svg dist/nodes/",
    "prepare": "npm run build"
  },
  "dependencies": {
    "imapflow": "^1.0.144",
    "nodemailer": "^6.9.13"
  },
  "devDependencies": {
    "n8n-core": "^1.64.0",
    "n8n-workflow": "^1.64.0",
    "typescript": "^5.4.0",
    "@types/nodemailer": "^6.4.15"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uberoptix/ionos-smtp.git"
  },
  "bugs": {
    "url": "https://github.com/uberoptix/ionos-smtp/issues"
  },
  "homepage": "https://github.com/uberoptix",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "n8n": {
    "credentials": [
      "dist/credentials/IMapCredentials.credentials.js",
      "dist/credentials/SmtpCredentials.credentials.js"
    ],
    "nodes": [
      "dist/nodes/IonosImap.node.js"
    ]
  }
}

