{
  "name": "n8n-nodes-imap-manager",
  "version": "0.3.8",
  "description": "n8n node to manage IMAP mail: search by Message-ID, add keywords, move, copy, delete.",
  "author": "James Forwood",
  "license": "MIT",
  "keywords": [
    "n8n-community-node",
    "imap",
    "imap-manager",
    "imapflow"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p tsconfig.json",
    "postbuild": "cp nodes/imap-manager.svg dist/nodes/"
  },
  "dependencies": {
    "imapflow": "^1.0.144",
    "n8n-core": "^1.64.0",
    "n8n-workflow": "^1.64.0"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uberoptix/ionos-smtp.git"
  },
  "bugs": {
    "url": "https://github.com/uberoptix/ionos-smtp/issues"
  },
  "homepage": "https://github.com/uberoptix",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "n8n": {
    "credentials": [
      "dist/credentials/IMapCredentials.credentials.js"
    ],
    "nodes": [
      "dist/nodes/IonosImap.node.js"
    ]
  }
}
